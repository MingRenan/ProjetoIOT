// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") // VocÃª define isso no .env
}

model Umidade {
  id        Int      @id @default(autoincrement())
  umidade   Int
  criadoEm  DateTime @default(now())
}
